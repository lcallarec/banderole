{
    "extends": "google",
    "plugins": [
        "jest"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jest": true

    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "max-len": [
            1,
            100
        ]
    }
}