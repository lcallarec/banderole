{
    "extends": "google",
    "parser": "babel-eslint",
    "plugins": [
        "jest",
        "flowtype"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es6": true

    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "max-len": [
            1,
            100
        ]
    }
}